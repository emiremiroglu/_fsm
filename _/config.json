{
  "stackadapt": {
    "url": "https://api.stackadapt.com/service/v2"
  },
  "puppeteer": {
    "headless": "new",
    "args": [
      "--no-sandbox",
      "--disable-setuid-sandbox",
      "--unhandled-rejections=strict"
    ]
  },
  "pdf": {
    "bufferPages": true,
    "margins": {
      "left": 0,
      "right": 0,
      "top": 0,
      "bottom": 0
    },
    "layout": "landscape"
  }
}